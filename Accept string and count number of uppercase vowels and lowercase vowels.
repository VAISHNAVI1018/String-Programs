#include<stdio.h>
#include<string.h>
int main()
{
  char s1[100];
  int i,c1=0,c2=0;
  printf("enter string");
  fgets(s1,100,stdin);
  printf("\n vowels");
  for(i=0;s1[i]!='\0';i++)
  {
    if(s1[i]=='o'||s1[i]=='e'||s1[i]=='a'||s1[i]=='i'||s1[i]=='u')
     c1++;
    else if(s1[i]=='A'||s1[i]=='E'||s1[i]=='I'||s1[i]=='O'||s1[i]=='U')
     c2++;
  }
   printf("\n lowercase vowel=%d",c1);
   printf("\n uppercase vowel=%d",c2);
}
